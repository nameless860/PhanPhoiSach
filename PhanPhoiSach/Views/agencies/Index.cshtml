@model IEnumerable<PhanPhoiSach.Models.agency>

@{
    ViewBag.Title = "Đại lý";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Đại lý</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.agency_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.agency_address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.agency_phone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.agency_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.agency_address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.agency_phone)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.agency_id }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.agency_id })
        </td>
    </tr>
}

</table>
